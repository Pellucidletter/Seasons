function dispalyGreetings(today){
        hrs = today.getHours();
        name="Arianna"
        if (hrs < 12)
            greet = 'Good Morning  '+Arianna;
        else if (hrs >= 12 && hrs <= 17)
            greet = 'Good Afternoon '+Arianna;
        else if (hrs >= 17 && hrs <= 24)
            greet = 'Good Evening  '+Arianna;
        document.getElementById('greet').innerHTML = greet;
